<?xml version="1.0"?>
<configuration>
<system.webServer>
<rewrite>
  <rules>
    <rule name="Index MySQL|MSSQL/IIS Version without parameters" stopProcessing="true">
      <match url="^$" ignoreCase="false" />
      <action type="Rewrite" url="mysql-version.php" />
    </rule>
    <rule name="Index MySQL|MSSQL/IIS Version with parameters" stopProcessing="true">
      <match url="(.*)" ignoreCase="false" />
      <conditions>
        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" ignoreCase="false" negate="true" />
      </conditions>
      <action type="Rewrite" url="mysql-version.php?page={R:1}" appendQueryString="true" />
    </rule>
  </rules>
</rewrite>
</system.webServer>
</configuration>
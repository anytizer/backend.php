<?xml version="1.0"?>
<configuration>
<system.webServer>
<rewrite>
<rules>
<!--# Do not allow to access any files directly. -->
<rule name="Protect all sub-domain files for __SUBDOMAIN_NAME__" stopProcessing="true">
<match url="(.*)" ignoreCase="false" />
<action type="Rewrite" url="index.php?file={R:1}" appendQueryString="true" />
</rule>
</rules>
</rewrite>
</system.webServer>
</configuration>
